{
  "name": "sap-cloud-cmis-client",
  "description": "A CMIS client built with @sap-cloud-sdk tools",
  "version": "1.0.0",
  "author": {
    "name": "VinÃ­cius Barrionuevo",
    "url": "https://github.com/vneecious"
  },
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/vneecious/sap-cloud-cmis-client"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.0"
  },
  "keywords": [
    "sap",
    "dms",
    "cmis"
  ],
  "bugs": {
    "url": "https://github.com/vneecious/sap-cloud-cmis-client/issues"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint ./src/ --fix",
    "lint:check": "eslint ./src/",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test": "jest --coverage --config jest.config.js --setupFiles dotenv/config",
    "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
    "format:check": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --check"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@sap-cloud-sdk/openapi-generator": "3.2.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "semantic-release": "^21.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3.4.0",
    "@sap-cloud-sdk/core": "^1.54.2",
    "@sap-cloud-sdk/http-client": "^3.4.0",
    "@sap-cloud-sdk/openapi": "^3.4.0",
    "form-data": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "type": "build",
            "scope": "deps",
            "release": "patch"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "build",
              "section": "Dependencies and Other Build Updates",
              "hidden": false
            }
          ]
        }
      }
    ],
    "@semantic-release/npm",
    "@semantic-release/github",
    [
      "@semantic-release/git",
      {
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}
